# LangGraph API Configuration
LANGGRAPH_API_KEY=

# OpenAI API Key (if using OpenAI models)
OPENAI_API_KEY=

# Gmail Fetcher Configuration (for fetching emails via IMAP)
# Uses app password authentication for charlesparmar@gmail.com
GMAIL_FETCHER_ADDRESS=charlesparmar@gmail.com
GMAIL_FETCHER_APP_PASSWORD=your_gmail_app_password_here

# Gmail Sender Configuration (for sending emails via OAuth 2.0)
# Uses OAuth 2.0 authentication for charles@parmarcharles.com
GMAIL_ADDRESS=charles@parmarcharles.com
GMAIL_API_CREDENTIALS_PATH=credentials.json
GMAIL_API_TOKEN_PATH=token_sender.json

# Supabase API Configuration (for REST API)
SUPABASE_API_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Pushover Configuration (for notifications)
PUSHOVER_USER_KEY=your_pushover_user_key_here
PUSHOVER_TOKEN=your_pushover_app_token_here

# SQLite Cloud Configuration
SQLITE_API_KEY=your_sqlite_cloud_api_key_here

# Email Configuration (for report sending)
# Note: Reports are sent FROM charles@parmarcharles.com (OAuth)
EMAIL_TO=charlesparmar@gmail.com
EMAIL_CC=charlesparmar@gmail.com
EMAIL_SUBJECT_PREFIX=Charles Parmar : Progress Report
EMAIL_SEND_NOTIFICATIONS=true

# RAG Pipeline Configuration
# Environment
RAG_ENVIRONMENT=development
RAG_DEBUG=false

# Database Configuration (for fitness data)
DB_HOST=localhost
DB_PORT=5000
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_NAME=fitness_data

# Vector Store Configuration
VECTOR_STORE_TYPE=chromadb
VECTOR_COLLECTION=fitness_embeddings
VECTOR_PERSIST_DIR=./chroma_db
EMBEDDING_MODEL=text-embedding-ada-002

# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-3.5-turbo
LLM_API_KEY=your_llm_api_key_here
LLM_API_BASE=

# Cache Configuration
CACHE_ENABLED=true
CACHE_RESPONSE_SIZE=1000
CACHE_VECTOR_SIZE=500

# Optimization Configuration
OPTIMIZATION_ENABLED=true
MAX_CONCURRENT_REQUESTS=10

# Web Interface Configuration
WEB_INTERFACE_ENABLED=true
WEB_HOST=0.0.0.0
WEB_PORT=8080
WEB_SECRET_KEY=your_web_secret_key_here

# Monitoring Configuration
MONITORING_ENABLED=true
LOG_LEVEL=INFO
LOG_FILE=./logs/rag_pipeline.log

# Security Configuration
AUTH_ENABLED=false
JWT_SECRET=your_jwt_secret_here
RATE_LIMITING_ENABLED=true

# Integration Configuration
AUTO_SYNC_ENABLED=true
SYNC_INTERVAL_MINUTES=30

# Other configuration
ENVIRONMENT=development 